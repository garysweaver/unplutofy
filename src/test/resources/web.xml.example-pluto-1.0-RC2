<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <display-name>someportlet</display-name>
    <description>Some description</description>
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>             /WEB-INF/context/portlet/file1.xml,             /WEB-INF/context/portlet/file2.xml,             /WEB-INF/context/portlet/file3.xml,             /WEB-INF/context/portlet/file4.xml,             /WEB-INF/context/portlet/file5.xml         </param-value>
    </context-param>
    <context-param>
        <param-name>log4jConfigLocation</param-name>
        <param-value>/WEB-INF/classes/log4j.properties</param-value>
    </context-param>
    <context-param>
        <param-name>webAppRootKey</param-name>
        <param-value>some.portlet.package</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.util.Log4jConfigListener</listener-class>
    </listener>
    <listener>
        <listener-class>org.springframework.web.util.WebAppRootListener</listener-class>
    </listener>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>jsp.WEB_002dINF.jsp.example1_jsp</servlet-name>
        <servlet-class>jsp.WEB_002dINF.jsp.example1_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>jsp.WEB_002dINF.jsp.example2_jsp</servlet-name>
        <servlet-class>jsp.WEB_002dINF.jsp.example2_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>jsp.WEB_002dINF.jsp.example3_jsp</servlet-name>
        <servlet-class>jsp.WEB_002dINF.jsp.example3_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>jsp.WEB_002dINF.jsp.example4_jsp</servlet-name>
        <servlet-class>jsp.WEB_002dINF.jsp.example4_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>jsp.WEB_002dINF.jsp.example5_jsp</servlet-name>
        <servlet-class>jsp.WEB_002dINF.jsp.example5_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewRendererServlet</servlet-name>
        <servlet-class>org.springframework.web.servlet.ViewRendererServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>someportlet</servlet-name>
        <display-name>someportlet Wrapper</display-name>
        <description>Automated generated Portlet Wrapper</description>
        <servlet-class>org.apache.pluto.core.PortletServlet</servlet-class>
        <init-param>
            <param-name>portlet-class</param-name>
            <param-value>org.springframework.web.portlet.DispatcherPortlet</param-value>
        </init-param>
        <init-param>
            <param-name>portlet-guid</param-name>
            <param-value>someportlet.someportlet</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>jsp.WEB_002dINF.jsp.example6_jsp</servlet-name>
        <servlet-class>jsp.WEB_002dINF.jsp.example6_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>jsp.WEB_002dINF.jsp.example7_jsp</servlet-name>
        <servlet-class>jsp.WEB_002dINF.jsp.example7_jsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>jsp.WEB_002dINF.jsp.example8_jsp</servlet-name>
        <servlet-class>jsp.WEB_002dINF.jsp.example8_jsp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>jsp.WEB_002dINF.jsp.example1_jsp</servlet-name>
        <url-pattern>/WEB-INF/jsp/example1.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>jsp.WEB_002dINF.jsp.example2_jsp</servlet-name>
        <url-pattern>/WEB-INF/jsp/example2.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>jsp.WEB_002dINF.jsp.example3_jsp</servlet-name>
        <url-pattern>/WEB-INF/jsp/example3.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>jsp.WEB_002dINF.jsp.example4_jsp</servlet-name>
        <url-pattern>/WEB-INF/jsp/example4.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>jsp.WEB_002dINF.jsp.example5_jsp</servlet-name>
        <url-pattern>/WEB-INF/jsp/example5.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewRendererServlet</servlet-name>
        <url-pattern>/WEB-INF/servlet/view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>someportlet</servlet-name>
        <url-pattern>/someportlet/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>jsp.WEB_002dINF.jsp.example6_jsp</servlet-name>
        <url-pattern>/WEB-INF/jsp/example6.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>jsp.WEB_002dINF.jsp.example7_jsp</servlet-name>
        <url-pattern>/WEB-INF/jsp/example7.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>jsp.WEB_002dINF.jsp.example8_jsp</servlet-name>
        <url-pattern>/WEB-INF/jsp/example8.jsp</url-pattern>
    </servlet-mapping>
    <taglib>
        <taglib-uri>http://java.sun.com/portlet</taglib-uri>
        <taglib-location>/WEB-INF/tld/portlet.tld</taglib-location>
    </taglib>
</web-app>
